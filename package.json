{
  "name": "complete-intro-to-react-v5",
  "version": "1.0.0",
  "description": "the Complete Intro to React, v5",
  "main": "src/App.js",
  "homepage": "",
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/core": "^10.0.28",
    "@frontendmasters/pet": "^1.0.3",
    "@reach/router": "^1.3.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@testing-library/react": "^10.4.8",
    "@types/reach__router": "^1.3.5",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "express": "^4.17.1",
    "jest": "^26.2.2",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "redux-mock-store": "^1.5.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "build-resources": "parcel build src/index.html --no-cache",
    "build-server-resources": "parcel build src/index-server.html --no-cache --public-url /public/",
    "start": "npm run build-server-resources && babel-node server/index.js",
    "clear-build-cache": "npm run clear-build-cache-cache && npm run clear-build-cache-dist",
    "clear-build-cache-cache": "if exist \"./.cache\" rmdir \"./.cache\" /Q /S",
    "clear-build-cache-dist": "if exist \"./dist\" rmdir \"./dist\" /Q /S",
    "clear-build-cache-nm": "if exist \"./node_modules\" rmdir \"./node_modules\" /Q /S",
    "dev": "parcel src/index.html",
    "dev:mock": "cross-env PET_MOCK=mock parcel src/index.html",
    "format": "prettier --write \"src/**/*.{js,jsx,tsx,ts}\"",
    "lint": "tslint --project && eslint \"src/**/*.{js,jsx}\" --quiet",
    "test": "jest",
    "test:update": "jest -u",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "author": "Brian Holt <btholt+complete-intro-to-react@gmail.com>",
  "license": "Apache-2.0",
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
