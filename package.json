{
  "name": "complete-intro-to-react-v5",
  "version": "1.0.0",
  "description": "the Complete Intro to React, v5",
  "main": "src/App.js",
  "homepage": "",
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/core": "^10.0.28",
    "@frontendmasters/pet": "^1.0.3",
    "@reach/router": "^1.3.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@types/reach__router": "^1.3.5",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "build-resources": "parcel build src/index.html --no-cache",
    "build-server-resources": "parcel build src/index-server.html --no-cache --public-url /public/",
    "start": "npm run build-server-resources && babel-node server/index.js",
    "clear-build-cache": "npm run clear-build-cache-1 && npm run clear-build-cache-2",
    "clear-build-cache-1": "if exist \"./.cache\" rmdir \"./.cache\" /Q /S",
    "clear-build-cache-2": "if exist \"./dist\" rmdir \"./dist\" /Q /S",
    "dev": "parcel src/index.html",
    "dev:mock": "cross-env PET_MOCK=mock parcel src/index.html",
    "format": "prettier --write \"src/**/*.{js,jsx,tsx,ts}\"",
    "lint": "tslint --project",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Brian Holt <btholt+complete-intro-to-react@gmail.com>",
  "license": "Apache-2.0",
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
